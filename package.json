{
  "name": "yomuki-blog",
  "version": "1.0.0",
  "description": "",
  "author": "yomuki",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "prepare": "husky install",
    "lint:lint-staged": "lint-staged",
    "commit": "git-cz",
    "cli:add": "pnpm run cli:add --filter yomuki-blog-cli"
